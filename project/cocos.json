{
    "engineDir" : "../node_modules/",
    "dependencies" : [
		"socketioclient",
        "cocos2d-html5",
		"cctouch",
		"cceditbox"
    ],

    "genRes" : {
        "output" : "cfg/res.js",
        "fileTypes" : [
            "png", "jpg", "bmp", "jpeg", "gif", "mp3", "ogg", "wav", "mp4", "plist",
            "xml", "fnt", "tmx", "tsx", "ccbi", "font", "txt", "vsh", "fsh", "json", "ExportJson"
        ],
        "dirCfgs" : ["res/Graphics->res/Graphics", "res/Audio->res/Audio"]
    },

    "genJsRes" : {
        "output" : "cfg/jsRes.js",
        "fileTypes" : ["js"],
        "dirCfgs" : ["src", "test"]
    },

    "genBaseCfg" : {
        "output" : "baseCfg.js"
    },

    "publish" : {
        "output" : "mini.js",
        "compilationLevel" : "advanced",
        "warning" : "quiet",
        "useSourceMap" : true,
        "sourceMapOutputFile" : "sourcemap",
        "sourceMapFormat" : "V3",
        "debug" : false,
        "delLog" : false
    }
}